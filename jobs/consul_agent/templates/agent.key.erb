<%=
  if p("consul.agent.mode") == "client"
    agent_key = nil

    respond_to?(:if_link) && if_link("consul_client") do |link|
      agent_key = link.p("consul.agent_key")
    end.else do
      respond_to?(:if_link) && if_link("consul") do |link|
        agent_key = link.p("consul.agent_key")
      end.else do
        agent_key = p("consul.agent_key")
      end
    end

    agent_key
  end
%>

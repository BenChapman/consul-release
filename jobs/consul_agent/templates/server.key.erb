<%=
  if p("consul.agent.mode") == "server"
    server_key = nil

    respond_to?(:if_link) && if_link("consul_server") do |link|
      server_key = link.p("consul.server_key")
    end.else do
      respond_to?(:if_link) && if_link("consul") do |link|
        server_key = link.p("consul.server_key")
      end.else do
        server_key = p("consul.server_key")
      end
    end

    server_key
  end
%>
